#include<iostream>
using namespace std;

int main(){
    /* Read input as specified in the question.
     * Print output as specified in the question.
     */
    
    int i,n;
    cin>>n;
    int input[n];
    int sum=0;
    for(i=0;i<n;i++){
        cin>>input[i];
    }
    for(i=0;i<n;i++){
        sum=sum+input[i];
        
    }
    cout<<sum<<endl;
}
